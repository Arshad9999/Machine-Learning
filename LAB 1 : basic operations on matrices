
import numpy as np
x = np.array([[1,1,1],[1,1,1],[1,1,2]])
y = np.array([[1,3,1],[1,3,1],[1,3,8]])
add = np.add(x,y)
print("Addition of two Matrices : \n",add)
subtract = np.subtract(x,y)
print("Subtraction of two Matrices : \n",subtract) multiply = np.multiply(x,y)
print("Mutliplication of two Matrices : \n",multiply)
